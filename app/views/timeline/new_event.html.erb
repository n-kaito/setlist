<h3>イベントを追加</h3>
<%= form_for @event,:url => {:action => :create_event} do |f| %>
  <%= render :partial => 'form', :locals =>{:f => f}  %>


  <%= link_to '/timeline', :class=>'btn btn-danger' do %>
    <i class="icon-arrow-left"></i> キャンセル
  <% end %>

  <%= f.submit '作成', :class => 'btn btn-primary' %>
  
  <!--
  <i class="icon-pencil"></i> 作成
  -->
<% end %>
